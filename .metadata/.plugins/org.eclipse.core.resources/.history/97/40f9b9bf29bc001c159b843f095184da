server.port=8180

spring.datasource.driverclassName=com.mysql.jdbc.Driver
spring.datasource.url=jdbc:mysql://db:3306/dados?useTimezone=true&serverTimezone=UTC&useSSL=false
spring.datasource.username=root
spring.datasource.password=docker

# JPA config

spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQLInnoDBDialect
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=false

#Config Flyway

spring.flyway.url=jdbc:mysql://db:3306/dados
spring.flyway.schemas=dados
spring.flyway.user=root
spring.flyway.password=docker

# config jackson

spring.jackson.default-property-inclusion=non-null

# Config upload e dowload file
spring.servlet.multipart.enabled=true
spring.servlet.multipart.file-size-threshold=2KB
spring.servlet.multipart.max-file-size=200MB
spring.servlet.multipart.max-request-size=215MB

file.upload-dir=/temp








